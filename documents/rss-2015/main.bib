@article{rabinovich2007oc,
	author = {Rabinovich, A.  and Vedaldi, A.  and Galleguillos, C.  and Wiewiora, E.  and Belongie, S. },
	citeulike-article-id = {2846012},
	comment = {(private-note)In this work the authors use semantic object context as a post processing step given an object recognition system.  They use a conditional random field to maximize label agreement according to contextual relevance.  The properties of objects in 1) interposition 2)support 3) position and 4) famaliar size has been addressed by the vision community, but 5)probability (objects tend to be found in some contexts and not others) has not been addressed as much.  The authors first segment the images, run classification to get a list of N labels for each segment, and then use context to figure out the most likely set of labels from the candidates.  To perform the classification, nearest neighbors is performed and the class with the lowest l\_1 distance is taken.

To incorporate semantic context, the authors use a fully connected CRF between segment labels.  In addition, they train a CRF on simpler problems on a relatively small number of segments.  They use google sets and also the Pascal dataset.  Hoewever, at the assignment stage, it does not appear that they use color or feature information directly from the image.},
	journal = {Computer Vision, 2007. ICCV 2007. IEEE 11th International Conference on},
	keywords = {context, objectrecognition, segmentation},
	pages = {1--8},
	posted-at = {2008-05-30 00:18:15},
	priority = {3},
	title = {{Objects in Context}},
	year = {2007}
}

